PROGRAM _a01;
DATA 
    INT32_t _a11, _b11, _c11, _m11;

START
    READ (_a11) 
    READ (_b11) 
    READ (_c11)

    IF (_a11 GE _b11) AND (_a11 GE _c11) {
        _a11 :> _m11
    } ELSE IF (_b11 GE _a11 ) AND (_b11 GE _c11) {
        _b11 :> _m11 
    } ELSE {
        _c11 :> _m11 
    }
    WRITE (_m11)

    IF (_a11  = _b11) AND (_a11 = _c11) AND (_b11 = _c11) {
        WRITE (1)
    } ELSE {
        WRITE (0)
    }

    IF !(_a11  GE 0) | !(_b11 GE 0) | !(_c11 GE 0) {
        WRITE (SUB 1)
    } ELSE {
        WRITE (0)
    }

    IF (_a11 GE (_b11 ADD _c11)) {
        WRITE 10
    } ELSE {
        WRITE 0
    }

FINISH




